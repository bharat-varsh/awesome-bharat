{
    "name": "awesome-bharat-static",
    "version": "1.0.0",
    "description": "Static site for Awesome Bharat",
    "type": "module",
    "scripts": {
        "dev": "astro dev",
        "build": "astro check && astro build --outDir ./docs",
        "preview": "astro preview --outDir ./docs",
        "astro": "astro",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write \"**/*.{js,ts,mjs,astro,md,mdx,json}\"",
        "prebuild": "npm run format && npm run lint",
        "build.local": "npm run build && npm run preview"
    },
    "dependencies": {
        "@astrojs/mdx": "^4.0.0",
        "@astrojs/rss": "^4.0.9",
        "@astrojs/sitemap": "^3.2.3",
        "@astrojs/tailwind": "^5.1.3",
        "astro": "^5.16.6",
        "tailwindcss": "^3.4.17"
    },
    "devDependencies": {
        "@astrojs/check": "^0.9.6",
        "@eslint/js": "^9.18.0",
        "@tailwindcss/typography": "^0.5.16",
        "@types/node": "^22.10.5",
        "eslint": "^9.18.0",
        "eslint-plugin-astro": "^1.3.2",
        "globals": "^15.14.0",
        "lightningcss": "^1.30.2",
        "prettier": "^3.4.2",
        "prettier-plugin-astro": "^0.14.1",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.20.0"
    }
}
